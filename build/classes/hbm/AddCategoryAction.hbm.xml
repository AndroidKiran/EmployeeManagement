<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
		
		<hibernate-mapping>
		
			<class name="com.pojo.masters.AddCategoryActionPojo" table="ADD_CATEGORY">
			
				<id name="seq_no" column="SEQ_NO" type="java.lang.Integer">
					<generator class="increment"></generator>				
				</id>
			
				<property name="cat_id" column="CAT_ID" type="java.lang.String"></property>
				<property name="cat_name" column="CAT_NAME" type="java.lang.String"></property>
				
			</class>
		
		</hibernate-mapping>
