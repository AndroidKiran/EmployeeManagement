<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
		
		<hibernate-mapping>
		
			<class name="com.pojo.medicalRepresentative.TourPlanningActionPojo" table="TOUR_PLAN">
			
				<id name="seq_no" column="SEQ_NO" type="java.lang.Integer">
					<generator class="increment"></generator>				
				</id>
			
				<property name="tourPurpose" column="TOURPURPOSE" type="java.lang.String" ></property>
				<property name="tourDateDB" column="TOURDATE" type="java.util.Date"></property>				
				<property name="descDoc" column="DESC_DOC" type="java.lang.String"></property>				
				<property name="time" column="TIME" type="java.lang.String"></property>				
				<property name="descChem" column="DESC_CHEM" type="java.lang.String"></property>				
				<property name="userName" column="USERNAME" type="java.lang.String"></property>				
				<property name="isCompleted" column="ISCOMPLETED" type="java.lang.Boolean"></property>	
				<property name="empId" column="EMPID" type="java.lang.String"></property>			
				<property name="createdDate" column="CREATED_DATE" type="java.util.Date"></property>			
				
				
							
			</class>
		
		</hibernate-mapping>
