<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
		
		<hibernate-mapping>
		
			<class name="com.pojo.masters.WorkTypeActionPojo" table="WORK_TYPE">
			
				<id name="workTypeId" column="WORK_TYPE_ID" type="java.lang.Integer">
					<generator class="increment"></generator>				
				</id>
			
				<property name="workTypeName" column="WORK_TYPE_NAME" type="java.lang.String"></property>
				
			</class>
		
		</hibernate-mapping>
